{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableRateLimiting",
    ":rebaseStalePrs"
  ],
  "major": {
    "enabled": false
  },
  "hostRules": [
    {
      "matchHost": "https://api.github.com/repos/input-output-hk",
      "hostType": "github",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "docker",
      "matchHost": "ghcr.io",
      "username": "MonikaJassova",
      "encrypted": {
        "password": ""
      }
    }
  ],
  "nix": {
    "enabled": true
  },
  "docker-compose": {
    "fileMatch": [
      "(^|/)(?:docker-)?compose[^/]*\\.ya?ml$"
    ]
  },
  "lockFileMaintenance": { 
    "enabled": true 
  },
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["ghcr.io/midnight-ntwrk/midnight-node"],
      "versioning": "semver"
    }
  ]
}
