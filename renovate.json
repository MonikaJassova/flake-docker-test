{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableRateLimiting",
    ":rebaseStalePrs"
  ],
  "hostRules": [
    {
      "matchHost": "https://api.github.com/repos/input-output-hk",
      "hostType": "github",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "docker",
      "matchHost": "ghcr.io/midnight-ntwrk",
      "username": "MonikaJassova",
      "password": "{{ secrets.GH_TOKEN }}"
    }
  ],
  "nix": {
    "enabled": true
  },
  "docker-compose": {
    "fileMatch": [
      "(^|/)(?:docker-)?compose[^/]*\\.ya?ml$"
    ]
  },
  "lockFileMaintenance": { 
    "enabled": true 
  }
}
